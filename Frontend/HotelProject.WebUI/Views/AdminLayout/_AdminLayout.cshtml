


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Quixlab - Bootstrap Admin Dashboard Template by Themefisher.com</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/adminweb/images/favicon.png">
    <!-- Custom Stylesheet -->
    <link href="~/adminweb/css/style.css" rel="stylesheet">

</head>

<body>
    @await Html.PartialAsync("/Views/AdminLayout/PreloaderPartial.cshtml")

    <div id="main-wrapper">
        @await Html.PartialAsync("/Views/AdminLayout/NavheaderPartial.cshtml")
        @await Html.PartialAsync("/Views/AdminLayout/HeaderPartial.cshtml")
        @await Html.PartialAsync("/Views/AdminLayout/SiderbarPartial.cshtml")
        @await Html.PartialAsync("/Views/AdminLayout/FooterPartial.cshtml")
        @await Html.PartialAsync("/Views/AdminLayout/ScriptPartial.cshtml")     
        <div class="content-body">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()
                                <br />
                                @await Html.PartialAsync("/Views/AdminLayout/HeadPartial.cshtml")
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
@RenderBody()
